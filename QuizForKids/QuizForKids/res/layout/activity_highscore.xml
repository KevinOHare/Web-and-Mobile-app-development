<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
	android:background="@drawable/highscoresbackground"
    tools:context="com.example.quizforkids.Highscore" >
    
    <TextView 
        android:id="@+id/Level1Highscore"
        android:textSize="30sp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="190sp"
        android:paddingTop="125sp"
        />
    
       <TextView 
        android:id="@+id/Level2Highscore"
        android:textSize="30sp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="190sp"
        android:paddingTop="230sp"
        />
        
       <TextView 
        android:id="@+id/Level3Highscore"
        android:textSize="30sp"
        android:textColor="#ffffff"
        android:textStyle="bold"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="190sp"
        android:paddingTop="335sp"
        />

</RelativeLayout>
